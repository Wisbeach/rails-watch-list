
<div class="text-center text-white">
    <h1>Movie List Maker</h1>
    </div>
<div class="container p-5">
<h1>Your lists</h1>

  <ul>
    <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list) %></li>
    <% end %>
  </ul>
  <%= link_to "Create a new list!", new_list_path %>
  </div>
